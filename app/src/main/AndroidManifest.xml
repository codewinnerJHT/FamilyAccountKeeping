<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.haitao.familyaccountkeeping">

    <application
        android:allowBackup="true"
        android:icon="@drawable/login_title"
        android:label="智能记账"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".AcountMain"></activity>
        <activity android:name=".AccountNew"></activity>
        <activity android:name=".AccountOperation"></activity>
        <activity android:name=".AccountSetting"></activity>
        <activity android:name=".AccountSearch"></activity>
        <activity android:name=".AccountData"></activity>
        <activity android:name=".AccountDataPay"></activity>
        <activity android:name=".AccountForget"></activity>
    </application>

</manifest>